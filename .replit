modules = ["python-3.11", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["web_scraper==1.0.0", "python_openai==1.0.0", "python_database==1.0.0", "python_log_in_with_replit==1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["c-ares", "cacert", "cargo", "glibcLocales", "grpc", "libiconv", "libuv", "libxcrypt", "oneDNN", "openssl", "pkg-config", "re2", "rustc", "util-linux", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Viamigo Flask Server"

[[workflows.workflow]]
name = "Viamigo Flask Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main_flask.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8000
externalPort = 80

[deployment]
deploymentTarget = "cloudrun"
run = ["sh", "-c", "python3 run.py"]
